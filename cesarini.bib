@InCollection{reser11,  author = 	 {Mezzanzanica, M. and Cesarini, M. and Boselli, R.},  title = 	 {Public Service Intelligence: evaluating how the Public Sector can exploit Decision Support Systems},  booktitle = 	 "RESER 2011, Productivity of Services NextGen  - Beyond Output / Input",  editor = {Ganz, W. and Kicherer, F. and Schletz, A.},  year = 	 {2011},  pdf =          {copyrightedpdf/reser12.pdf},  isbn =         {9783839602980},  pages = {1--18},  publisher = {Fraunhofer VERLAG},  abstract = {This paper aims at exploring how ICTs can improve decision-making processes and subsequently service performances in public sector services. The authors focus on a specific class of Information Systems: Decision Support Systems (DSS) and Business Intelligence (BI). The research presented will lay the ground for investigating how the impact of these technologies on public services can be evaluated. The paper identifies the key factors influencing BI and DSS adoption and value generation in the public sector, focusing on a literature review. Furthermore, drawing on the identified key factors, the paper proposes a framework with the aim of identifying specific public service sectors where BI and DSS adoption could provide effective results.},  keywords = {public sector, business intelligence, decision support systems, ICT impact, service improvement},  address = {Hamburg, Germay}}
@incollection {semantic10,   author = {Celino, Irene and Cerizza, Dario and Cesarini, Mirko and Valle, Emanuele Della and Paoli, Flavio and Estublier, Jacky and Fugini, MariaGrazia and P\'{e}rez, Asuncion G\'{o}mez and Kerrigan, Mick and Guarrera, Pascal and Mezzanzanica, Mario and Ram\`{i}rez, Jaime and Villazon, Boris and Zhao, Gang},   title = {SEEMP: A Networked Marketplace for Employment Services},   booktitle = {Semantic Technologies for E-Government},   editor = {Vitvar, Tomas and Peristeras, Vassilios and Tarabanis, Konstantinos},   publisher = {Springer Berlin Heidelberg},   isbn = {978-3-642-03507-4},   keyword = {Computer Science},   pages = {151-183},   url = {http://dx.doi.org/10.1007/978-3-642-03507-4_7},   doi = {10.1007/978-3-642-03507-4_7},   abstract = {Human capital is more and more the key factor of economic growth and competitiveness in the information age and knowledge economy. But due to a still fragmented employment market compounded by the enlargement of the EU, the human resources are not effectively exchanged and deployed. The business innovation of SEEMP 1 develops a vision of an Employment Mediation Marketplace (EMM) for market transparency and effic ient mediation. Its technological innovation provides a federated marketplace of employment agencies through a peer-to-peer network of employment data and mediation services. In other words, the solution under development is a de-fragmentation of the employment market by a web-based collaborative network. The SEEMP-enabled employment marketplace will strengthen the social organization of public employment administration, maximize the business turnover of private employment agencies, improve citizens' productivity and welfare, and increase the competitiveness and performance of business.},   year = {2010}}
